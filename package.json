{
  "name": "tailwindcss",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env PORT=3001 NODE_ENV=development nodemon server/index.js --watch server",
    "test": "cross-env NODE_ENV=testing node server/index.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "build": "nuxt build",
    "build-dev": "cross-env NODE_ENV=development nuxt build",
    "build-test": "cross-env NODE_ENV=testing nuxt build",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@gauseen/nuxt-proxy": "^1.0.1",
    "@nuxtjs/axios": "^5.13.6",
    "bq-static": "^1.1.6",
    "core-js": "^3.19.3",
    "cross-env": "^7.0.3",
    "daisyui": "^2.3.7-alpha.0",
    "koa": "^2.13.4",
    "node-sass": "^4.14.1",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-cookie": "^1.1.4",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^5.0.0-4",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "sass-loader": "^10.2.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "svg-sprite-loader": "^6.0.11",
    "tailwindcss": "^3.0.23"
  }
}
